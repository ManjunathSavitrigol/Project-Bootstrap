@{
    ViewBag.Title = "Home Page";
}

<style>
    tr {
        font-weight: bolder;
    }

        tr td:first-child {
            width: 100px;
        }

        tr td:nth-child(2) {
            width: 25px;
        }

    .maincard {
        height: 400px;
        width: 350px;
    }
</style>
<main>
    @*accordian*@
    <div class="accordion" id="accordion">
        <div class="accordion-item" data-bs-parent="accordion">
            <div class="accordion-header">
                <button class="accordion-button" data-bs-toggle="collapse" data-bs-target="#accordion-o-carousel"><h1>Carousel</h1></button>
            </div>
            <div class="accordion-collapse collapse show" id="accordion-o-carousel" data-bs-parent="#accordion">
                <div class="accordion-body">
                    @*carousel*@
                    <div class="accordion" id="accordion-carousel">
                        <div class="accordion-item" data-bs-parent="accordion-carousel">
                            <div class="accordion-header">
                                <button class="accordion-button" data-bs-toggle="collapse" data-bs-target="#accordion-carousel-intro"><h2>Example</h2></button>
                            </div>
                            <div class="accordion-collapse collapse show" id="accordion-carousel-intro" data-bs-parent="#accordion-carousel">
                                <div class="accordion-body">
                                    <div class="carousel slide bg-black mb-3" id="carousel" data-bs-ride="carousel" data-bs-interval="3000">
                                        <div class="carousel-indicators">
                                            <button type="button" data-bs-target="#carousel" data-bs-slide-to="0" class="active" aria-current="true"></button>
                                            <button type="button" data-bs-target="#carousel" data-bs-slide-to="1"></button>
                                            <button type="button" data-bs-target="#carousel" data-bs-slide-to="2"></button>
                                        </div>
                                        <div class="carousel-inner">
                                            <div class="carousel-item active">
                                                <div style="height:100vh;width:100vw">
                                                    <img src="~/Content/Images/alex-shuper-Or-ysyrPXnU-unsplash.jpg" class="h-100 w-100" style="object-fit:contain" />
                                                </div>
                                                <div class="carousel-caption">
                                                    <h1> Carousel</h1>
                                                    <p> Carousel is nothing but a slide show. </p>
                                                </div>
                                            </div>

                                            <div class="carousel-item ">
                                                <div style="height:100vh;width:100vw">
                                                    <img src="~/Content/Images/jaromir-kavan-JL4LDwMd5rA-unsplash.jpg" class="h-100 w-100" style="object-fit:contain" />
                                                </div>
                                                <div class="carousel-caption">
                                                    <h1> Carousel features</h1>
                                                    <p> In carousel we have dark colors. And we can control slide timing and fading-animation. </p>
                                                </div>
                                            </div>

                                            <div class="carousel-item  ">
                                                <div style="height:100vh;width:100vw">
                                                    <img src="~/Content/Images/ft-shafi-yXj3_8hhKQM-unsplash.jpg" class="h-100 w-100" style="object-fit:contain" />
                                                </div>
                                                <div class="carousel-caption">
                                                    <h1> Carousel components</h1>
                                                    <p> Indicators, content, previous and next buttons </p>
                                                </div>
                                            </div>
                                        </div>
                                        <button type="button" class="carousel-control-prev" data-bs-slide="prev" data-bs-target="#carousel">
                                            <span class="carousel-control-prev-icon"></span>
                                        </button>
                                        <button type="button" class="carousel-control-next" data-bs-slide="next" data-bs-target="#carousel">
                                            <span class="carousel-control-next-icon"></span>
                                        </button>
                                    </div>
                                    <div class="row">
                                        <div class="col">
                                            <button class="btn btn-danger" id="carousel-next"> Next </button>
                                        </div>
                                        <div class="col">
                                            <button class="btn btn-danger" id="carousel-prev"> Previous </button>
                                        </div>
                                        <div class="col">
                                            <button class="btn btn-danger" id="carousel-pause"> Pause </button>
                                        </div>
                                        <div class="col">
                                            <div class="input-group">
                                                <input class="form-control" id="carousel-to-input" type="number" />
                                                <button class="btn btn-danger" id="carousel-to">Go</button>
                                            </div>
                                        </div>
                                        <div class="col">
                                            <button class="btn btn-danger" id="carousel-dispose">Dispose</button>
                                        </div>
                                        <div class="col">
                                            <button class="btn btn-danger" id="carousel-eslid">Event slid.bs.carousel</button>
                                        </div>
                                        <div class="col">
                                            <button class="btn btn-danger" id="carousel-eslide">Event slide.bs.carousel</button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                        <div class="accordion-item">
                            <div class="accordion-header">
                                <button class="accordion-button collapsed" data-bs-toggle="collapse" data-bs-target="#accordion-carousel-javascript"><h2>Via Javascript</h2></button>
                            </div>
                            <div class="accordion-collapse collapse" id="accordion-carousel-javascript" data-bs-parent="#accordion-carousel">
                                <div class="accordion-body">
                                    <p>We can control carousel via javascript.</p>
                                    <p>
                                        <b>
                                            var myCarousel = document.querySelector('#myCarousel')<br />
                                            var carousel = new bootstrap.Carousel(myCarousel, Options)
                                        </b>
                                    </p>
                                </div>
                            </div>

                        </div>
                        <div class="accordion-item">
                            <div class="accordion-header">
                                <button class="accordion-button collapsed" data-bs-toggle="collapse" data-bs-target="#accordion-carousel-option"><h2>Options</h2></button>
                            </div>
                            <div class="accordion-collapse collapse" id="accordion-carousel-option" data-bs-parent="#accordion-carousel">
                                <div class="accordion-body">
                                    <table class="table">
                                        <tbody>
                                            <tr>
                                                <td>Interval </td>
                                                <td>-</td>
                                                <td>Number (Interval between two slides)</td>
                                            </tr>
                                            <tr>
                                                <td>Keyboard </td>
                                                <td>-</td>
                                                <td>Boolean</td>
                                            </tr>
                                            <tr>
                                                <td>Pause </td>
                                                <td>-</td>
                                                <td>String, Boolean (hover(default), mouseleave, mouseenter, true, false)</td>
                                            </tr>
                                            <tr>
                                                <td>Ride </td>
                                                <td>-</td>
                                                <td>String, Boolean (Autoplays the carousel after the user manually cycles the first item. If set to 'carousel', autoplays the carousel on load.)</td>
                                            </tr>
                                            <tr>
                                                <td>Wrap </td>
                                                <td>-</td>
                                                <td>Boolean (	Whether the carousel should cycle continuously or have hard stops.)</td>
                                            </tr>
                                            <tr>
                                                <td>Touch </td>
                                                <td>-</td>
                                                <td>Boolean (Whether the carousel should support left/right swipe interactions on touchscreen devices.)</td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>

                        </div>
                        <div class="accordion-item">
                            <div class="accordion-header">
                                <button class="accordion-button collapsed" data-bs-toggle="collapse" data-bs-target="#accordion-carousel-methods"><h2>Javascript Methods</h2></button>
                            </div>
                            <div class="accordion-collapse collapse" id="accordion-carousel-methods" data-bs-parent="#accordion-carousel">
                                <div class="accordion-body">
                                    <table class="table">
                                        <tbody>
                                            <tr>
                                                <td>cycle</td>
                                                <td>-</td>
                                                <td>Cycles through carousel items form left to right</td>
                                            </tr>
                                            <tr>
                                                <td>pause</td>
                                                <td>-</td>
                                                <td>Stops the carousel from cycling through</td>
                                            </tr>
                                            <tr>
                                                <td>prev</td>
                                                <td>-</td>
                                                <td>Cycles to the previous item</td>
                                            </tr>
                                            <tr>
                                                <td>next</td>
                                                <td>-</td>
                                                <td>Cycles to the next item</td>
                                            </tr>
                                            <tr>
                                                <td>nextWhenVisible</td>
                                                <td>-</td>
                                                <td>Dont cycle through the carousel when the page or its parent is not visible.</td>
                                            </tr>
                                            <tr>
                                                <td>to</td>
                                                <td>-</td>
                                                <td>Cycles to the specified carousel item.</td>
                                            </tr>
                                            <tr>
                                                <td>dispose</td>
                                                <td>-</td>
                                                <td>Destroys an element's carousel. (Removes stored data on the DOM element)</td>
                                            </tr>
                                            <tr>
                                                <td>getInstance</td>
                                                <td>-</td>
                                                <td>Gets the instance of the carousel.</td>
                                            </tr>
                                            <tr>
                                                <td>getOrCreateInstance</td>
                                                <td>-</td>
                                                <td>Gets the instance of the carousel, if not present then creates a carousel instance.</td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>

                        <div class="accordion-item">
                            <div class="accordion-header">
                                <button class="accordion-button collapsed" data-bs-target="#accordion-carousel-events" data-bs-toggle="collapse"><h2>Events</h2></button>
                            </div>
                            <div class="accordion-collapse collapse" id="accordion-carousel-events" data-bs-parent="#accordion-carousel">
                                <div class="accordion-body">
                                    <table class="table">
                                        <tbody>
                                            <tr>
                                                <td>slide.bs.carousel</td>
                                                <td>-</td>
                                                <td>Fires immediately when the slide instance method is invoked.</td>
                                            </tr>
                                            <tr>
                                                <td>slid.bs.carousel</td>
                                                <td>-</td>
                                                <td>Fired when the carousel has completed its slide transition.</td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                    @*end*@
                </div>
            </div>

        </div>
        <div class="accordion-item">
            <div class="accordion-header">
                <button class="accordion-button collapsed" data-bs-toggle="collapse" data-bs-target="#accordion-o-accodion"><h1>Accordion</h1></button>
            </div>
            <div class="accordion-collapse collapse" id="accordion-o-accodion" data-bs-parent="#accordion">
                <div class="accordion-body">
                    <div class="accordion" id="accordion-accordion">
                        <div class="accordion-item">
                            <div class="accordion-header">
                                <button class="accordion-button collapsed" data-bs-toggle="collapse" data-bs-target="#accordion-accordion-intro"><h2>Example</h2></button>
                            </div>
                            <div class="accordion-collapse collapse" id="accordion-accordion-intro" data-bs-parent="#accordion-accordion">
                                <div class="accordion-body">
                                    <div class="accordion" id="accordion-ex">
                                        <div class="accordion-item">
