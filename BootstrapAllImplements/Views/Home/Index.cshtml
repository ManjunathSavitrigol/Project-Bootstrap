<script type="text/javascript">

    let breakpoints = ["sm", "md", "lg", "xl", "xxl"];
    let sizes = ["lg", "sm", ""];
    let colors = ["primary", "secondary", "danger", "success", "warning", "info", "light", "dark"];


    //-------------------carousel javascript---------------------------
    //next
    $("#carousel-next").click(function () {
        $("#carousel").carousel('next');
    })

    //previous
    $("#carousel-prev").click(function () {
        $("#carousel").carousel('prev');
    })

    //pause
    $("#carousel-pause").click(function () {
        $("#carousel").carousel('pause');
    })

    //to
    $("#carousel-to").click(function () {
        let toval = $("#carousel-to-input").val();
        toval = parseInt(toval);
        $("#carousel").carousel(toval);
    })

    //pause
    $("#carousel-dispose").click(function () {
        $("#carousel").carousel('dispose');
    })

    //events
    function alerting() {
        alert("I am alerted before the sliding was initiated.")
        carousel.removeEventListener('slide.bs.carousel', alerting);
    }

    function alerting2() {
        alert("I am alerted after the sliding is completed.")
        carousel.removeEventListener('slid.bs.carousel', alerting2);
    }

    //slid.bs.carousel
    $("#carousel-eslid").click(function () {
        let carousel = document.getElementById("carousel");
        $("#carousel").carousel('next');
        carousel.addEventListener('slid.bs.carousel', alerting2)
    })

    //slide.bs.carousel
    $("#carousel-eslide").click(function () {
        let carousel = document.getElementById("carousel");
        $("#carousel").carousel('next');
        carousel.addEventListener('slide.bs.carousel', alerting)
    })

    //-----------------------------end--------------------------------------

    //-------------------------Alerts Javascript----------------------------

    //-------------------------Badge Javascript----------------------------
    let badgecolorid = 0;
    $("#badge-change-color").click(function () {
        let badge = document.getElementById("badge");

        if (badgecolorid == 8) {
            badgecolorid = 0;
        }
        console.log(colors[badgecolorid]);
        if (badgecolorid == 0) {
            badge.classList.remove("bg-" + colors[7]);
        }
        else {

            badge.classList.remove("bg-" + colors[badgecolorid - 1]);
        }
        badge.classList.add("bg-" + colors[badgecolorid]);
        badgecolorid++;
    })

    //-------------------------Button Javascript----------------------------
    //change color
    let button = document.getElementById("button");
    let buttoncolorid = 0;

    $("#button-color").click(function () {

        //remove outline class
        button.classList.forEach((value) => {
            if (value.includes("outline")) {
                button.classList.remove(value);
            }
        })

        if (buttoncolorid == 8) {
            buttoncolorid = 0;
        }

        if (buttoncolorid == 0) {
            button.classList.remove("btn-" + colors[7]);
        }
        else {

            button.classList.remove("btn-" + colors[buttoncolorid - 1]);
        }

        button.classList.add("btn-" + colors[buttoncolorid]);
        buttoncolorid++;

    })

    //change to outline
    $("#button-outline").click(function () {
        button.classList.remove("btn-" + colors[buttoncolorid - 1]);
        button.classList.add("btn-outline-" + colors[buttoncolorid - 1]);
    })

    //change size
    let buttonsizeid = 0;
    $("#button-size").click(function () {

        if (buttonsizeid == 2) {
            buttonsizeid = 0;
        }

        if (buttonsizeid == 0) {
            button.classList.remove("btn-" + sizes[1]);
        }
        else {

            button.classList.remove("btn-" + sizes[buttonsizeid - 1]);
        }
        button.classList.add("btn-" + sizes[buttonsizeid]);
        buttonsizeid++;
    })


    //-------------------------Modal Javascript----------------------------
    let modal = new bootstrap.Modal(document.getElementById("modal-simple"), {});
    $("#modal-input-btn").click(function () {
        let modalvalue = $("#modal-input").val();
        console.log(modalvalue);
        if (modalvalue == 3) {
            modal.show();

            setTimeout(function () { modal.hide(); }, 5000)
        }

    })

    //-------------------------Offcanvas Javascript----------------------------
    let offcanvas = new bootstrap.Offcanvas(document.getElementById("offcanvas-backdrop"));

    //toggle
    $("#offcanvas-toggle").click(function () {
        offcanvas.toggle();
    })

    //show
    $("#offcanvas-show").click(function () {
        offcanvas.show();
    })

    //hide
    $("#offcanvas-hide").click(function () {
        offcanvas.hide();
    })

    //-------------------------Popover Javascript----------------------------
    var popoverTriggerList = [].slice.call(document.querySelectorAll('[data-bs-toggle="popover"]'))
    var popoverList = popoverTriggerList.map(function (popoverTriggerEl) {
        return new bootstrap.Popover(popoverTriggerEl)
    })

    //close popover on close of accordion on open and close of the parent element.
    $(".accordion-button").click(function () {
        popoverTriggerList.map(function (eachpopover) {
            var pops = bootstrap.Popover.getOrCreateInstance(eachpopover);
            pops.hide();
        })
    })

    //-------------------------Progressbar Javascript----------------------------
    var progresswidth = 0;
    var t = setInterval(function () {
        var progressbarelement = document.getElementById("jsprogressbar");
        progressbarelement.style.width = (progresswidth++) + "%";
        progressbarelement.innerText = (progresswidth) + "%";

        //change color on 50%
        if (progresswidth == 50) {
            progressbarelement.classList.add("bg-success");
        }
        if (progresswidth == 100) {
            progresswidth = 0;
            progressbarelement.classList.remove("bg-success");
        }
    }, 1000);


//-------------------------Toasts Javascript----------------------------
    var toastElList = [].slice.call(document.querySelectorAll('.toast'));
    var toastList = toastElList.map(function (toastEl) {
        return new bootstrap.Toast(toastEl, {
            autohide: true
        })
    })

    $("#liveToastBtn").click(function () {
        toastList.forEach(element => {
            element.show();
        });
    })

     

</script>